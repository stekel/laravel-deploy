{
    "name": "stekel/laravel-deploy",
    "description": "Helpful command for deploying a given repository to a remote machine.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">7.1",
        "illuminate/console": "^5.7 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.7 || ^6.0 || ^7.0 || ^8.0",
        "phpseclib/phpseclib": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "stekel\\LaravelDeploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stekel\\LaravelDeploy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "stekel\\LaravelDeploy\\Laravel\\Providers\\LaravelDeployServiceProvider"
            ],
            "aliases": {}
        }
    }
}
